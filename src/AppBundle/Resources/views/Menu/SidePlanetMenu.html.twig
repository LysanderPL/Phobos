{% javascripts
'public/Bundles/App/js/sidePlanetMenu.min.js'

filter='?uglifyjs2' %}
<script src="{{ asset_url }}" type="text/javascript"></script>
{% endjavascripts %}


<div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 mainBodyContentBox">
    <br/>
    <ul class="nav nav-pills nav-stacked">
        {% for menu_position in menu_positions %}
            <li role="presentation" id="{{ menu_position.clear_path }}"><a
                        href="{{ menu_position.path|e }}">{{ menu_position.name|e }}</a></li>
        {% endfor %}
    </ul>
    <br/>
</div>

<script type="application/javascript">SidePlanetMenu.Run();</script>